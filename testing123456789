        ["Tempest"] = Vector3.new(37, 132, 1945),
        ["Windset"] = Vector3.new(2850, 180, 2700),
        ["Smokescreen"] = Vector3.new(2790, 140, -630),
        ["Aurora"] = Vector3.new(-1811, -137, -3282),
    }

    -- Define Rods
local Rods = {
