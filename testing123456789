local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "FischHelper",
    SubTitle = "by neonbeon",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = false,
    Theme = "Darker",
    MinimizeKey = Enum.KeyCode.LeftControl
})

local Tabs = {
    NPC = Window:AddTab({ Title = "NPC", Icon = "user" }),
    Items = Window:AddTab({ Title = "Items", Icon = "box"}),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

local Options = Fluent.Options

do
    Fluent:Notify({
        Title = "WARNING!!!",
        Content = "Use at your own risk",
        SubContent = "",
        Duration = 10
    })

    Tabs.NPC:AddParagraph({
        Title = "Teleports you to the NPC",
        Content = "Will teleport to the selected NPC"
    })

    Tabs.Items:AddParagraph({
        Title = "PLEASE READ",
        Content = [[For some of the items you are required to complete their tasks.
Example: You need to open the gates for trident and depths.]]
    })

    -- Define NPC locations for each island
    local Moosewood = {
        Phineas = Vector3.new(470, 151, 274),
