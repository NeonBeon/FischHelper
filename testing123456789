        Shipwright = Vector3.new(-2479, 134, 1543),
        Jack = Vector3.new(-2825, 215, 1518),
    }
    local Depths = {
        Angler = Vector3.new(979, -703, 1234),
        Merchant = Vector3.new(951, -712, 1263),
        Aspicientis = Vector3.new(1212, -707, 1320),
