    Aurora = Vector3.new(-144, -516, 1132),
    ["Depths (gate)"] = Vector3.new(1704, -903, 1442),
    Magnet = Vector3.new(-197, 133, 1931),
    Kings = Vector3.new(1380, -808, -303),
    Reinforce = Vector3.new(-991, -244, -2693),
    ["Trident (gate)"] = Vector3.new(-1480, -225, -2338),
    Scurvy = Vector3.new(-2826, 215, 1511),
    ["Destiny (quest)"] = Vector3.new(980, 132, -1234),
    ["Fungal (quest)"] = Vector3.new(2598, 133, -727),
}

local OtherItems = {
    Crabcage = Vector3.new(476, 151, 232),
    GPS = Vector3.new(513, 151, 282),
    Fishradar = Vector3.new(368, 135, 271),
    Basicdiving = Vector3.new(372, 135, 248),
    Conch = Vector3.new(-1632, -214, -2861),
    Glider = Vector3.new(-1643, -213, -2853),
    Tidebreaker = Vector3.new(-1643, -213, -2853),
    Coralgeode = Vector3.new(-1643, -213, -2853),
    Flippers = Vector3.new(-966, -245, -2699),
    Superflippers = Vector3.new(-966, -245, -2699),
    Advancediving = Vector3.new(-978, -255, -2689)
}

    -- Function to create dropdowns for a given tab
    local function createDropdown(tab, title, elements)
        local elementNames = {}
        for name, _ in pairs(elements) do
            table.insert(elementNames, name)
        end

        local dropdown = tab:AddDropdown(title, {
            Title = "Select " .. title,
            Values = elementNames,
            Multi = false,
            Default = nil,
        })

        dropdown:OnChanged(function(Value)
            local position = elements[Value]
            if position then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(position)
                print("Teleported to:", Value, position)
                dropdown:SetValue(nil)
            else
                print(title .. " not found:", Value)
            end
        end)
    end

    -- Create dropdowns for each set
    createDropdown(Tabs.NPC, "Moosewood", Moosewood)
    createDropdown(Tabs.NPC, "Mushgrove", Mushgrove)
    createDropdown(Tabs.NPC, "Roslit", Roslit)
    createDropdown(Tabs.NPC, "Snowcap", Snowcap)
    createDropdown(Tabs.NPC, "Sunstone", Sunstone)
    createDropdown(Tabs.NPC, "Terrapin", Terrapin)
    createDropdown(Tabs.NPC, "Statue", Statue)
    createDropdown(Tabs.NPC, "Arch", Arch)
    createDropdown(Tabs.NPC, "Vertigo", Vertigo)
    createDropdown(Tabs.NPC, "Desolate Deep", DesolateDeep)
    createDropdown(Tabs.NPC, "Desolate Pocket", DesolatePocket)
    createDropdown(Tabs.NPC, "Forsaken", Forsaken)
    createDropdown(Tabs.NPC, "Depths", Depths)
    createDropdown(Tabs.Items, "Totems", Totems)
    createDropdown(Tabs.Items, "Rods", Rods)
    createDropdown(Tabs.Items, "Other", OtherItems)

    -- Build settings section
    InterfaceManager:SetLibrary(Fluent)
    InterfaceManager:BuildInterfaceSection(Tabs.Settings)

    -- Select the first tab (NPC)
    Window:SelectTab(1)

    -- Notify the user that the script has loaded
    Fluent:Notify({
        Title = "FischHelper",
        Content = "The script has been loaded.",
        Duration = 8
    })
end
